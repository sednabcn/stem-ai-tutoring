// Storage Security Rules  
rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /documents/{allPaths=**} {
      allow read: if request.auth != null;
    }
    match /agreements/{tutorId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == tutorId;
    }
  }